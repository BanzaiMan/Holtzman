        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <name>NewSpring</name>
    <description>App for NewSpring Church</description>
    <author email="web@newspring.cc" href="https://newspring.cc">NewSpring Church</author>
    <universal-links>
        <ios-team-id value="7TM8C6252Z" />
        <host event="universalLinkRoute" name="beta.newspring.cc" scheme="https">
            <path url="/articles" />
            <path url="/articles/*" />
            <path url="/devotionals" />
            <path url="/devotionals/*" />
            <path url="/studies" />
            <path url="/studies/*" />
            <path url="/studies/*/*" />
            <path url="/stories" />
            <path url="/stories/*" />
            <path url="/sermons" />
